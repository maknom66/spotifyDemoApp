{
  "name": "spotifydemoapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:dev": "react-native run-android --variant=development",
    "android:staging": "react-native run-android --variant=staging",
